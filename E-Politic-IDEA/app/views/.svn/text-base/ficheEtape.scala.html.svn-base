 @(etape: Etape )



<!--  Partie ligne des buttons -->
<div class="row panel panel-default">

	<div class="col-sm-10">
		<div class="row" style="padding: 4px; margin: 4px; float: left">


			<div class="col-sm-2">
				<a onclick="changeStateEtape(@etape.getId(), 'planifier')" href="#"><button
						class="btn btn-primary btn-xs" id="btnPlanifier">
						<span class="glyphicon glyphicon-calendar"></span> Planifier l'étape
					</button></a>
			</div>
			<div class="col-sm-2">
				<a onclick="changeStateEtape(@etape.getId(), 'demarre')" href="#"><button
						class="btn btn-info btn-xs" id="btnDemarrer">
						<span class="glyphicon glyphicon-play"></span> Démarrer l'étape
					</button></a>
			</div>

			<div class="col-sm-2">
				<a onclick="changeStateEtape(@etape.getId(), 'suspendre')" href="#"><button
						class="btn btn-warning btn-xs" id="btnSuspendre">
						<span class="glyphicon glyphicon-pause"></span> Suspendre l'étape
					</button></a>
			</div>

			<div class="col-sm-2">
				<a onclick="changeStateEtape(@etape.getId(), 'terminer')" href="#"  ><button 
						class="btn btn-success btn-xs" id="btnTerminer">
						<span class="glyphicon glyphicon-ok"></span> Terminer l'étape
					</button></a>
			</div>
			<div class="col-sm-2">
				<a onclick="changeStateEtape(@etape.getId(), 'abandonner')" href="#"><button
						class="btn btn-danger btn-xs" id="btnAbandonner">
						<span class="glyphicon glyphicon-stop"></span> Abandonner l'étape
					</button></a>
			</div>
			<div class="col-sm-2">
				<button class="btn btn-info btn-xs" data-toggle="modal"
					data-target="#UpdateFicheEtape" id="btnModifier">
					<span class="glyphicon glyphicon-pencil"></span> Modifier l'étape
				</button>
			</div>
		</div>
	</div>

	<div class="col-sm-2">
		<div class="row pull-right" style="padding: 4px; margin: 4px; float: left">

			<a href="@routes.ProjetCtrl.infoProjet(etape.getProjet().getId())"
				style="color: black" class=""> <span
				class="glyphicon glyphicon-folder-open">  </span> Projet
			</a>
		</div>

	</div>
</div>











<!--  Partie ligne du corps -->

<div class="row ">

	<!--  Partie colonne info 1 -->
	<div class="col-sm-6 panel-group ">

		<div class="row">
			<div class="panel panel-etape">
				<div id="panelDetailsEtape" class="mypanel-heading hover-link "
					data-toggle="collapse" data-parent="false" data-target="#collapse1">

					<div class="row">
						<div class="col-sm-10">
							<span class=""></span>
							<h4 class="panel-title">
								<span id="spanDetailsEtape"
									class="glyphicon glyphicon-chevron-up"></span> Details
							</h4>
						</div>
						<div class="col-sm-2">
							<a href="#" class="pull-right  " data-toggle="modal"
								style="color: black" data-target="#UpdateDetailsEtape"
								id="btnModifierDetails"><span
								class="glyphicon glyphicon-pencil"></span></a>
						</div>
					</div>

				</div>
				<div id="collapse1" class="panel-collapse collapse in">
					<div class="panel-body">


						<div class="row">
							<div class="col-sm-5">
								<label>Nom de l'étape:</label>
							</div>
							<div class="col-sm-5">@etape.getNomEtape()</div>
						</div>

						<div class="row">
							<div class="col-sm-5">
								<label>Description  de l'étape:</label>
							</div>
							<div class="col-sm-5">@etape.getDescEtape()</div>
						</div>

						<div class="row">
							<div class="col-sm-5">
								<label>Etat  de l'étape:</label>
							</div>
							<div class="col-sm-5">
								@if(etape.getEtat()==Etape.STATE.EN_COURS){
								<div class="badge badge-info" role="alert">Etape en cours
									. . .</div>
								} @if(etape.getEtat()==Etape.STATE.SUSPENDU){
								<div class="badge badge-warning" role="alert">Etape
									suspendu</div>
								} @if(etape.getEtat()==Etape.STATE.TERMINE){
								<div class="badge badge-success" role="alert">Etape
									terminé</div>
								} @if(etape.getEtat()==Etape.STATE.DEMARRE){
								<div class="badge badge-info" role="alert">Etape demarré</div>
								} @if(etape.getEtat()==Etape.STATE.PLANIFIE){
								<div class="badge badge-primary" role="alert">Etape
									planifié</div>
								} @if(etape.getEtat()==Etape.STATE.ABANDONNE){
								<div class="badge badge-danger" role="alert">Etape
									abandonné</div>
								}
								
								@if(etape.getEtat()==Etape.STATE.EN_ATTENTE){
								<div class="badge" role="alert">Etape
									en attente</div>
								}
							</div>
						</div>

					</div>
				</div>
			</div>

		</div>

		<div class="row">
			<div class="panel panel-etape">
				<div id="panelEmploisEtape" class="mypanel-heading hover-link"
					data-toggle="collapse" data-parent="false" data-target="#collapse8">
					<div class="row">
						<div class="col-sm-10">
							<h4 class="panel-title">
								<span id="spanEmploisEtape"
									class="glyphicon glyphicon-chevron-up"></span> Emplois
							</h4>
						</div>
						<div class="col-sm-2">
							<a href="#" class="pull-right" data-toggle="modal"
								style="color: black" data-target="#UpdateEmploisEtape"
								id="btnModifierEmplois"><span
								class="glyphicon glyphicon-pencil"></span></a>
						</div>
					</div>
				</div>
				<div id="collapse8" class="panel-collapse collapse in">
					<div class="panel-body">


						<div class="row">
							<div class="col-sm-5">
								<label>Nombre d'emploi prévisionnel: </label>
							</div>
							<div class="col-sm-5">
								<div class="badge badge-info" role="alert">@etape.getEmploiPrevisionel()</div>
							</div>
						</div>



						<div class="row">
							<div class="col-sm-5">
								<label>Nombre d'emploi réel: </label>
							</div>
							<div class="col-sm-5">
								<div class="badge badge-info" role="alert">@etape.getEmploiReel()
								</div>
							</div>
						</div>

					</div>
				</div>
			</div>

		</div>
		<div class="row">
			<div class="panel panel-etape">
				<div id="panelTimesEtape" class="mypanel-heading hover-link"
					data-toggle="collapse" data-parent="false" data-target="#collapse6">
					<div class="row">
						<div class="col-sm-10">
							<h4 class="panel-title">
								<span id="spanTimesEtape" class="glyphicon glyphicon-chevron-up"></span>
								Time
							</h4>
						</div>
						<div class="col-sm-2">
							<a href="#" class="pull-right" data-toggle="modal"
								style="color: black" data-target="#UpdateNiveauEtape"
								id="btnModifierNiveauEtape"><span
								class="glyphicon glyphicon-pencil"></span></a>
						</div>
					</div>
				</div>
				<div id="collapse6" class="panel-collapse collapse in">
					<div class="panel-body">


						<div class="row">
							<div class="col-sm-5">
								<label>Niveaux d'éxecution  de l'étape:</label>
							</div>
							<div class="col-sm-5">
								<div class="progress">
									<div class="progress-bar" role="progressbar" aria-valuenow="60"
										aria-valuemin="0" aria-valuemax="100"
										style="width: @etape.getNiveauExecution()%;">@etape.getNiveauExecution()%</div>
								</div>
							</div>
						</div>


					</div>
				</div>
			</div>

		</div>

	</div>


	<!--  Partie colonne info 2 -->
	<div class="col-sm-6 panel-group  ">
		<div class="row">
			<div class="panel panel-etape">
				<div id="panelPersonnesEtape" class="mypanel-heading hover-link"
					data-toggle="collapse" data-parent="false" data-target="#collapse4">
					<div class="row">
						<div class="col-sm-10">
							<h4 class="panel-title">
								<span id="spanPersonnesEtape"
									class="glyphicon glyphicon-chevron-up"></span> Personnes
							</h4>
						</div>
						<div class="col-sm-2">
							<a href="#" class="pull-right" data-toggle="modal"
								style="color: black" data-target="#UpdatePersonnesEtape"
								id="btnModifierPersonnes"><span
								class="glyphicon glyphicon-pencil"></span></a>
						</div>
					</div>
				</div>
				<div id="collapse4" class="panel-collapse collapse in">
					<div class="panel-body">


						<div class="row">
							<div class="col-sm-5">
								<label>Autorité  de l'étape:</label>
							</div>
							<div class="col-sm-5">
								<span class="glyphicon glyphicon-user">
									@etape.getAutorite() </span>
							</div>
						</div>

						<div class="row">
							<div class="col-sm-5">
								<label>Contact de l'étape:</label>
							</div>
							<div class="col-sm-5">
								<span class="glyphicon glyphicon-user">
									@etape.getContatact() </span>
							</div>
						</div>





					</div>
				</div>
			</div>

		</div>
		<div class="row">
			<div class="panel panel-etape">
				<div id="panelDatesEtape" class="mypanel-heading hover-link"
					data-toggle="collapse" data-parent="false" data-target="#collapse5">
					<div class="row">
						<div class="col-sm-10">
							<h4 class="panel-title">
								<span id="spanDatesEtape" class="glyphicon glyphicon-chevron-up"></span>
								Dates
							</h4>
						</div>
						<div class="col-sm-2">
							<a href="#" class="pull-right" data-toggle="modal"
								style="color: black" data-target="#UpdateDatesEtape"
								id="btnModifierDates"><span
								class="glyphicon glyphicon-pencil"></span></a>
						</div>
					</div>
				</div>
				<div id="collapse5" class="panel-collapse collapse in">
					<div class="panel-body">


						<div class="row">
							<div class="col-sm-5">
								<span class="glyphicon glyphicon-time"></span> <label>
									Date de debut  de l'étape:</label>
							</div>
							<div class="col-sm-5">
								@etape.getDateDebut().format("dd/MM/yyyy")</div>
						</div>

						<div class="row">
							<div class="col-sm-5">
								<span class="glyphicon glyphicon-time"></span> <label>
									Date de fin  de l'étape:</label>
							</div>
							<div class="col-sm-5">@etape.getDateFin().format("dd/MM/yyyy")
							</div>
						</div>





					</div>
				</div>
			</div>

		</div>

		<div class="row">
			<div class="panel panel-etape">
				<div id="panelBudgetsEtape" class="mypanel-heading hover-link"
					data-toggle="collapse" data-parent="false" data-target="#collapse7">
					<div class="row">
						<div class="col-sm-10">
							<h4 class="panel-title">
								<span id="spanBudgetsEtape"
									class="glyphicon glyphicon-chevron-up"></span> Budget
							</h4>
						</div>
						<div class="col-sm-2">
							<a href="#" class="pull-right" data-toggle="modal"
								style="color: black" data-target="#UpdateBudgetsEtape"
								id="btnModifierBudgets"><span
								class="glyphicon glyphicon-pencil"></span></a>
						</div>
					</div>
				</div>
				<div id="collapse7" class="panel-collapse collapse in">
					<div class="panel-body">


						<div class="row">
							<div class="col-sm-5">
								<label>Cout prévisionnel: </label>
							</div>
							<div class="col-sm-5">
								<div class="badge badge-info" role="alert">@etape.getCoutPrevisionel()
								</div>
							</div>
						</div>

						<div class="row">
							<div class="col-sm-5">
								<label>Cout réel: </label>
							</div>
							<div class="col-sm-5">
								<div class="badge badge-info" role="alert">@etape.getCoutReel()</div>
							</div>
						</div>

					</div>
				</div>
			</div>

		</div>
	</div>
</div>

<!-- **************************************************************************************************************** -->
<!--Update Time -->
<div class="modal fade modal-admin" id="UpdateNiveauEtape" tabindex="-1"
	role="dialog" aria-labelledby="viewProfilModal" aria-hidden="true">
	<div class="col-md-4 col-lg-offset-4" id="">
		<div class="panel panel-info">
			<div class="panel-heading text-center">
				<h3 class="panel-title">Modifier un projet</h3>

			</div>
			<div class="panel-body" style="color: #000033">

				<form action="@routes.ProjetCtrl.updateNiveauExecutionEtape()"
					method="post" id="formUpdateNiveauEtape">
					<fieldset>
						<legend>Informations étape</legend>

						<input type="hidden" name="idEtape" id="" value="@etape.getId()"></input>

						<!-- Cout pr�visionnel -->
						<div class="row">
							<div class="col-md-4 text-left">
								<label for="editProfilNomInput">Niveau d'éxécution de
									l'étape </label>
							</div>
							<div class="col-md-6">
								<input type="number" min="0" class="form-control"
									required="required" name="niveauExecutionEtapeUp" id=""
									placeholder="Cout prévisionnel du projet"
									value="@etape.getNiveauExecution()" />
							</div>
						</div>
						<br>

					</fieldset>
			</div>
			<div class="panel-footer panel-footer-landing text-center">

				<button type="submit" id="bntSubmitUpdateNiveauEtape" onClick=""
					class="btn btn-info" value="submit">Enregisrer</button>
				<button type="reset" class=" annulerModifProfil btn btn-warning">Annuler
					Modifications</button>
				<button type="button" id="" data-dismiss="modal"
					class="btn btn-warning">Fermer</button>
			</div>
			</form>
		</div>
	</div>
</div>


<!--Update Details -->
<div class="modal fade modal-admin" id="UpdateDetailsEtape"
	tabindex="-1" role="dialog" aria-labelledby="viewProfilModal"
	aria-hidden="true">
	<div class="col-md-4 col-lg-offset-4" id="">
		<div class="panel panel-info">
			<div class="panel-heading text-center">
				<h3 class="panel-title">Modifier un Etape</h3>

			</div>
			<div class="panel-body" style="color: #000033">

				<form action="@routes.ProjetCtrl.updateDetailsEtape()" method="post"
					id="formUpdateDetailsEtape">
					<fieldset>
						<legend>Informations Etape</legend>

						<input type="hidden" name="idPro" value="@etape.getId()"></input>
						<!-- nom -->
						<div class="row">
							<div class="col-md-4  text-left">
								<label for="editProfilNomInput">Nom Etape: </label>
							</div>
							<div class="col-md-6">
								<input type="text" value="@etape.getNomEtape()"
									class="form-control" required="required" name="nomProUp"
									placeholder="Nom" id="" />
							</div>
						</div>
						<br>
						<!-- description -->
						<div class="row">
							<div class="col-md-4 text-left">
								<label for="editProfilNomInput">Description Etape: </label>
							</div>
							<div class="col-md-6">
								<input class="form-control" required="required"
									name="descriptionProUp" placeholder="description du Etape"
									id="" value="@etape.getDescEtape()" />
							</div>
						</div>

					</fieldset>
			</div>
			<div class="panel-footer panel-footer-landing text-center">

				<button type="submit" id="bntSubmitUpdateDetailsEtape" onClick=""
					class="btn btn-info" value="submit">Enregisrer</button>
				<button type="reset" class=" annulerModifProfil btn btn-warning">Annuler
					Modifications</button>
				<button type="button" id="" data-dismiss="modal"
					class="btn btn-warning">Fermer</button>
			</div>
			</form>
		</div>
	</div>
</div>

