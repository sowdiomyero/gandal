<!--Update Budgets -->
<div class="modal fade modal-admin" id="UpdateBudgetsEtape"
	tabindex="-1" role="dialog" aria-labelledby="viewProfilModal"
	aria-hidden="true">
	<div class="col-md-4 col-lg-offset-4" id="">
		<div class="panel panel-info">
			<div class="panel-heading text-center">
				<h3 class="panel-title">Modifier un Etape</h3>

			</div>
			<div class="panel-body" style="color: #000033">



				<form action="@routes.ProjetCtrl.updateBudgetsEtape()" method="post"
					id="formUpdateBudgetsEtape">
					<fieldset>
						<legend>Informations Etape</legend>

						<input type="hidden" name="idPro" value="@etape.getId()"></input>
						<input type="hidden" name="idLocPro" id=""></input>

						<!-- Cout prévisionnel -->
						<div class="row">
							<div class="col-md-4 text-left">
								<label for="editProfilNomInput">Cout prévisionnel  de l'étape: </label>
							</div>
							<div class="col-md-6">
								<input type="number" min="0" class="form-control"
									required="required" name="coutPrevisionnelProUp" id=""
									placeholder="Cout prévisionnel du Etape"
									value="@etape.getCoutPrevisionel()" />
							</div>
						</div>
						<br>
						<!-- Cout reel-->
						<div class="row">
							
							<div class="col-md-6">
								<input type="hidden" min="0" class="form-control"
									required="required" name="coutReelProUp"
									placeholder="Cout Reel de l'etape" value="@etape.getCoutReel()" />
							</div>
						</div>
