@(localite: Localite)
<div class="col-sm-3">
    <div class="panel panel-warning">
        <div class="panel-heading">
            <div class="row">
                <div class="col-xs-3">
                    <i class="fa fa-map-marker fa-2x"></i>
                </div>
                <div class="col-xs-9 text-right">
                    <div class="huge">@localite.NombrePopulations()</div>
                    <div></div>
                </div>
            </div>
        </div>
        <a href="#">
            <div class="panel-footer">
                <span class="pull-left">Populations</span>
                
                <div class="clearfix"></div>
            </div>
        </a>
    </div>

</div>
<div class="col-sm-3">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <div class="row">
                <div class="col-xs-3">
                    <i class="fa fa-users fa-2x"></i>
                </div>
                <div class="col-xs-9 text-right">
                    <div class="badge">@localite.NombreElecteurs()</div>
                    <div></div>
                </div>
            </div>
        </div>
        <a href="#">
            <div class="panel-footer">
                <span class="pull-left">Electeurs</span>
              
                <div class="clearfix"></div>
            </div>
        </a>
    </div>
</div>
<div class="col-sm-3">
    <div class="panel panel-warning">
        <div class="panel-heading">
            <div class="row">
                <div class="col-xs-3">
                    <i class="fa fa-tasks fa-2x"></i>
                </div>
                <div class="col-xs-9 text-right">
                    <div class="badge">@localite.NombreInscrits()</div>
                    <div></div>
                </div>
            </div>
        </div>
        <a href="#">
            <div class="panel-footer">
                <span class="pull-left">Inscrits</span>
              
                <div class="clearfix"></div>
            </div>
        </a>
    </div>
</div>
<div class="col-sm-3">
    <div class="panel panel-yellow">
        <div class="panel-heading">
            <div class="row">
                <div class="col-xs-3">
                    <i class="fa fa-list-ol fa-2x"></i>
                </div>
                <div class="col-xs-9 text-right">
                    <div class="badge">@localite.NombreSections()</div>
                    <div></div>
                </div>
            </div>
        </div>
        <a href="#">
            <div class="panel-footer">
                <span class="pull-left">Sections</span>
               
                <div class="clearfix"></div>
            </div>
        </a>
    </div>
</div>
